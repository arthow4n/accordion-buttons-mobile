{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./icons/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./icons/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.accordion.mobile",
      "adaptiveIcon": {
        "foregroundImage": "./icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "navigationBar": {
        "visible": "false",
        "backgroundColor": "#00000000"
      }
    },
    "web": {
      "favicon": "./icons/favicon.png"
    },
    "plugins": [
      "expo-asset",
      "./withAndroidDebugSigning"
    ]
  }
}